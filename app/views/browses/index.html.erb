<h1>Listing browses</h1>

<table border=1>
  <tr>
    <th>Name</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @browses.each do |browse| %>
      <tr>
        <td><%= browse.name %></td>
        <td><%= browse.content %></td>
        <td><%= link_to "Show", browse_path(browse.id) %></td>
        <td><%= link_to "Edit", edit_browse_path(browse.id) %></td>
      </tr>
  <% end %>
</table>

